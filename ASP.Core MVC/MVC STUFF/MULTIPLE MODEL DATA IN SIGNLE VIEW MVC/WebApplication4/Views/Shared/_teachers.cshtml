@model IEnumerable<WebApplication4.Models.teacher>
<h1>TEACHERS DATA</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>GENDER</th>
            <th>QUALIFICATION</th>
            <th>SALARY</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.name</td>
            <td>@item.gender</td>
            <td>@item.qualification</td>
            <td>@item.salary</td>
        </tr>
    }
</table>
